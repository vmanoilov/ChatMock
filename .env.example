# Port
PORT=8000

# Auth dir
CHATGPT_LOCAL_HOME=/data

# show request/stream logs
VERBOSE=false

# OAuth client id (modify only if you know what you're doing)
# CHATGPT_LOCAL_CLIENT_ID=app_EMoamEEZ73f0CkXaXp7hrann

# Reasoning controls
CHATGPT_LOCAL_REASONING_EFFORT=medium       # minimal|low|medium|high
CHATGPT_LOCAL_REASONING_SUMMARY=auto        # auto|concise|detailed|none
CHATGPT_LOCAL_REASONING_COMPAT=think-tags   # legacy|o3|think-tags|current

# Force a specific model name
# CHATGPT_LOCAL_DEBUG_MODEL=gpt-5

# Qwen provider configuration (use mock values for testing)
QWEN_AUTH_TOKEN="YOUR_QWEN_TOKEN_HERE"
QWEN_COOKIES="cookie1=value1; cookie2=value2; cookie3=value3"
QWEN_CHAT_ID="YOUR_CHAT_ID_HERE"
QWEN_MODEL="qwen3-max-preview"

# ChatMock configuration
CHATMOCK_BASE_PROMPT_PATH=""
CHATMOCK_RATE_LIMIT_RPS=8
CHATMOCK_QUEUE_TIMEOUT=120
CHATMOCK_LOG_LEVEL=info
CHATMOCK_CORS_ORIGINS="*"
CHATMOCK_REQUIRE_AUTH=false
CHATMOCK_ACCESS_TOKEN=""
INJECT_BASE_PROMPT=true
